  10 0000               #
  11 0000               # Description:	Add two ASCII numbers and store the result in ASCII.
  12 0000               #
  13 0000               # Arguments:	a0: address of parameter block.  The block consists of
  14 0000               #		four words that contain (in this order):
  15 0000               #
  16 0000               #			address of first input string
  17 0000               #			address of second input string
  18 0000               #			address where result should be stored
  19 0000               #			length of the strings and result buffer
  20 0000               #
  21 0000               #		(There is actually other data after this in the
  22 0000               #		parameter block, but it is not relevant to this routine.)
  23 0000               #
  24 0000               # Returns:	The result of the addition, in the buffer specified by
  25 0000               #		the parameter block.
  26 0000               #
  27 0000               
  28 0000               	.globl	add_ascii_numbers
  29 0000               
  30 0000               add_ascii_numbers:
  31 0000               A_FRAMESIZE = 40
  32 0000               
  33 0000               #
  34 0000               # Save registers ra and s0 - s7 on the stack.
  35 0000               #
  36 0000 23BDFFD8		addi 	$sp, $sp, -A_FRAMESIZE
  37 0004 AFBF0024		sw 	$ra, -4+A_FRAMESIZE($sp)
  38 0008 AFB7001C		sw 	$s7, 28($sp)
  39 000c AFB60018		sw 	$s6, 24($sp)
  40 0010 AFB50014		sw 	$s5, 20($sp)
  41 0014 AFB40010		sw 	$s4, 16($sp)
  42 0018 AFB3000C		sw 	$s3, 12($sp)
  43 001c AFB20008		sw 	$s2, 8($sp)
  44 0020 AFB10004		sw 	$s1, 4($sp)
  45 0024 AFB00000		sw 	$s0, 0($sp)
  46 0028               	
  47 0028               # ##### BEGIN STUDENT CODE BLOCK 1 #####
  48 0028               
  49 0028 8C900000	        lw      $s0, 0($a0)     # get address of first number
  50 002c 8C910004	        lw      $s1, 4($a0)     # get address of second number
  51 0030 8C920008	        lw      $s2, 8($a0)     # get address of result
  52 0034 8C93000C	        lw      $s3, 12($a0)    # get length of strings
  53 0038               
  54 0038 0000A021	        move    $s4, $zero      # initialize carry to 0
  55 003c               
  56 003c               addition_loop:
  57 003c                                               # exit when there are no more digits
  58 003c 1240000B	        beq     $s2, $zero, done_adding
  59 0040                       
  60 0040 0213A820	        add     $s5, $s0, $s3   # use digit $s3 away from beg of first num
  61 0044 0233B020	        add     $s6, $s1, $s3   # use digit $s3 away from beg of second num
  62 0048 0253B820	        add     $s7, $s2, $s3   # use digit #s3 away from beg of result
  63 004c               
  64 004c 82A90000	        lb      $t1, 0($s5)     # get the value of the digit in first number
  65 0050 82CA0000	        lb      $t2, 0($s6)     # get the value of the digit in second number
  66 0054               
  67 0054 012A5820	        add     $t3, $t1, $t2   # add two digits together
  68 0058 216BFFD0	        addi    $t3, $t3, -48   # subtract 48 to get correct ascii
  69 005c 01745820	        add     $t3, $t3, $s4   # add the carry
  70 0060               
  71 0060 A2EB0000	        sb      $t3, 0($s7)        # store the calulated digit in the result
  72 0064               
  73 0064 2273FFFF	        addi    $s3, $s3, -1    # decrement the digit pointer
  74 0068 0800000F	        j       addition_loop   # go back to beginning of loop
  75 006c               
  76 006c               done_adding:
  77 006c                               
  78 006c               
  79 006c               # ###### END STUDENT CODE BLOCK 1 ######
  80 006c               
  81 006c               #
  82 006c               # Restore registers ra and s0 - s7 from the stack.
  83 006c               #
  84 006c 8FBF0024		lw 	$ra, -4+A_FRAMESIZE($sp)
  85 0070 8FB7001C		lw 	$s7, 28($sp)
  86 0074 8FB60018		lw 	$s6, 24($sp)
  87 0078 8FB50014		lw 	$s5, 20($sp)
  88 007c 8FB40010		lw 	$s4, 16($sp)
  89 0080 8FB3000C		lw 	$s3, 12($sp)
  90 0084 8FB20008		lw 	$s2, 8($sp)
  91 0088 8FB10004		lw 	$s1, 4($sp)
  92 008c 8FB00000		lw 	$s0, 0($sp)
  93 0090 23BD0028		addi 	$sp, $sp, A_FRAMESIZE
  94 0094               
  95 0094 03E00008		jr	$ra			# Return to the caller.


A_FRAMESIZE        = 00000028   
add_ascii_numbers    00000000 RG text 
addition_loop        0000003C R  text 
done_adding          0000006C R  text 
